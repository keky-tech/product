{
    "name": "keky/product-module",
    "description": "Products management",
    "keywords": [
        "keky",
        "laravel",
        "product-module"
    ],
    "homepage": "https://github.com/keky/product-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Henoc Djabia",
            "email": "henoc35@gmail.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/keky-tech/services.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/keky-tech/query-master.git"
        }
    ],
    "require": {
        "php": "^8.2",
        "archtechx/enums": "^1.1",
        "illuminate/contracts": "^11.0||^12.0"
    },
    "require-dev": {
        "keky/query-master": "^0.1.1",
        "keky/services": "^0.1.1",
        "larastan/larastan": "^2.9||^3.0",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3||^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.1||^2.0",
        "phpstan/phpstan-phpunit": "^1.3||^2.0"
    },
    "autoload": {
        "psr-4": {
            "Keky\\Product\\": "src/",
            "Keky\\Product\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keky\\Product\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-product-module --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse --memory-limit=512M",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Keky\\Product\\ProductModule\\ProductModuleServiceProvider"
            ],
            "aliases": {
                "ProductModule": "Keky\\Product\\ProductModule\\Facades\\ProductModule"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
